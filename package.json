{
  "name": "figure-skating-trainer",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev:client": "vite",
    "client:with-seed": "VITE_ENABLE_SEED=true vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "tsc && tsc -p tsconfig.server.json --noEmit && vite build",
    "build:client": "tsc && tsc -p tsconfig.server.json --noEmit && vite build",
    "build:server": "tsc -p tsconfig.server.json --noEmit",
    "preview": "vite preview",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf dist dist-ssr .vite node_modules/.vite",
    "prebuild": "pnpm run clean",
    "generate-icons": "node scripts/generate-icons.js",
    "import:neon": "tsx scripts/importToNeon.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.0",
    "cors": "^2.8.5",
    "dexie": "^3.2.4",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.5",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "typescript-eslint": "^8.47.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.20.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^1.1.0"
  },
  "pnpm": {
    "overrides": {
      "source-map": "^0.7.4"
    }
  }
